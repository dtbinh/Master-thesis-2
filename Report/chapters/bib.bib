	Eprint = {1107.2487},
	File = {:Users/Maximilian/Documents/ETH/Master/Master thesis/1. Literature review/Provably safe and robust learning-based model predictive control.pdf:pdf},
	Isbn = {00051098},
	Issn = {00051098},
	Journal = {Automatica},
	Keywords = {Learning control,Predictive control,Robustness,Safety analysis,Statistics},
	Number = {5},
	Pages = {1216--1226},
	Publisher = {Elsevier Ltd},
	Title = {{Provably safe and robust learning-based model predictive control}},
